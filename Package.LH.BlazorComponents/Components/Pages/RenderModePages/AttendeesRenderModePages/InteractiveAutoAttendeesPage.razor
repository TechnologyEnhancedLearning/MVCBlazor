@page "/Attendees/InteractiveAuto-BlazorPage"
@using Package.LH.BlazorComponents.Layouts
@rendermode InteractiveAuto
@inherits GB_PageBase
@layout _LayoutBlazor 
@* layout LHB_RenderModePageLayout avoid layouts unless pure blazor not rendered by component mvc *@

